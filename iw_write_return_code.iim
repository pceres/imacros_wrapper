'input:
'RETURN_CODE 0 : return code to be written
'RETURN_MSG '' : last error msg if any
'
'SET RETURN_CODE 0
'SET RETURN_MSG ''

SET !EXTRACT {{TIMESTAMP}}
ADD !EXTRACT {{RETURN_CODE}}
ADD !EXTRACT {{RETURN_MSG}}
'PROMPT {{!EXTRACT}}
SAVEAS TYPE=EXTRACT FOLDER=* FILE={{DEST_FILENAME}}

' remove lock
FILEDELETE NAME={{CMD_FILENAME}}
